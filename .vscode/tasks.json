{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "npm: dev",
      "type": "shell",
      "command": "npm",
      "args": ["run", "dev"],
      "isBackground": true,
      "problemMatcher": {
        "pattern": {
          "regexp": "^.*$",
          "file": 1,
          "location": 2,
          "message": 3
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": "^.*VITE.*",
          "endsPattern": "^.*Local:.*"
        }
      },
      "runOptions": {
        "runOn": "default"
      }
    },
    {
      "label": "npm: build",
      "type": "shell",
      "command": "npm",
      "args": ["run", "build"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "npm: preview",
      "type": "shell",
      "command": "npm",
      "args": ["run", "preview"],
      "isBackground": true
    },
    {
      "label": "npm: deploy",
      "type": "shell",
      "command": "npm",
      "args": ["run", "deploy"]
    }
  ]
}
